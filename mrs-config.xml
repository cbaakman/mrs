<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mrs-config SYSTEM "http://mrs.cmbi.ru.nl/dtd/mrs-config.dtd">
<mrs-config>
  <!-- Paths where all the data and files are stored -->
  <directories>
    <directory id="mrs">__MRS_DATA_DIR__/mrs</directory>
    <directory id="raw">__MRS_DATA_DIR__/raw</directory>
    <directory id="log">__MRS_LOG_DIR__</directory>
    <directory id="blast">__MRS_DATA_DIR__/blast-cache</directory>
    <directory id="parser">__MRS_DATA_DIR__/parsers</directory>
    <directory id="docroot">__MRS_DATA_DIR__/docroot</directory>
  </directories>
  <!-- Tools section, specify external executables used by M6 -->
  <tools>
    <tool id="clustalo">__CLUSTALO__</tool>
    <tool id="rsync">__RSYNC__</tool>
  </tools>
  <!-- Scheduler section -->
  <scheduler enabled="true" time="20:00" weekday="friday"/>
  <!-- Users section, users can be added or modified using the command "mrs password" -->
  <users>
<!--    <user name="scott" realm="MRS Administrator" password="b92ff7d60b994c8a0d2941ad9349e841"/>-->
  </users>
  <!-- Server section -->
  <server addr="0.0.0.0" port="__MRS_PORT__" user="__MRS_USER__" pidfile="__MRS_RUN_DIR__/mrs.pid">
    <admin realm="MRS Administrator"/>
    <base-url>__MRS_BASE_URL__</base-url>
    <web-service service="mrsws_search" ns="http://mrs.cmbi.ru.nl/mrsws/search" location="mrsws/search"/>
    <web-service service="mrsws_blast" ns="http://mrs.cmbi.ru.nl/mrsws/blast" location="mrsws/blast"/>
  </server>
  <!-- Formats section, formats are used to add links to entries and
		 to link a JavaScript pretty printer -->
  <formats>
    <format id="omim" script="omim">
      <link an="$2" db="omim" id="$1" ix="id" rx="(?&lt;=\()(?:[^();]+?; )?(\d{6})(?:\.(\d{4}))?(?=\))"/>
    </format>
    <format id="uniprot" script="uniprot">
      <link db="embl" id="$1" ix="id" rx="(?&lt;=DR   EMBL; )(\S+)(?=;)"/>
      <link db="pdb" id="$1" ix="id" rx="(?&lt;=DR   PDB; )(\S+)(?=;)"/>
      <link db="refseq" id="$1" ix="id" rx="(?&lt;=DR   RefSeq; )(?:(\S+)\.\d+)(?=;)"/>
      <link db="unigene" id="$1" ix="id" rx="(?&lt;=DR   UniGene; )(\S+)(?=;)"/>
      <link db="go" id="$1" ix="id" rx="(?&lt;=DR   GO; )GO:(\S+)(?=;)"/>
      <link db="interpro" id="$1" ix="id" rx="(?&lt;=DR   InterPro; )(\S+)(?=;)"/>
      <link db="pfam" id="$1" ix="ac" rx="(?&lt;=DR   Pfam; )(\S+)(?=;)"/>
      <link db="prints" id="$1" ix="id" rx="(?&lt;=DR   PRINTS; )(\S+)(?=;)"/>
      <link db="prosite" id="$1" ix="ac" rx="(?&lt;=DR   PROSITE; )(\S+)(?=;)"/>
      <link db="omim" id="$1" ix="id" rx="(?&lt;=DR   MIM; )(\S+)(?=;)"/>
    </format>
    <format id="embl" script="embl"/>
    <format id="enzyme">
      <link db="sprot" id="$1" ix="ac" rx="([A-Z]+[A-Z0-9]+)(?=, )"/>
    </format>
    <format id="taxonomy">
      <link db="taxonomy" id="$1" ix="id" rx="(?&lt;=^PARENT ID                 : )(\d+)"/>
    </format>
    <format id="stockholm">
      <link db="pfama" id="$1" ix="id" rx="^(?:#=GF DR\s+PFAMA;\s)(\S+)(?=;)"/>
      <link db="pfamb" id="$1" ix="id" rx="^(?:#=GF DR\s+PFAMB;\s)(\S+)(?=;)"/>
      <link db="pdb" id="$1" ix="id" rx="^(?:#=GF DR\s+PDB;\s)(\S+)(?=;)"/>
      <link db="prosite" id="$1" ix="id" rx="^(?:#=GF DR\s+PROSITE;\s)(\S+)(?=;)"/>
      <link db="interpro" id="$1" ix="id" rx="^(?:#=GF DR\s+INTERPRO;\s)(\S+)(?=;)"/>
      <link db="uniprot" id="$1" ix="ac" rx="^(?:#=GS .+?AC )([0-9A-Z]+)"/>
      <link db="pdb" id="$1" ix="id" rx="^(?:#=GS .+?DR PDB; )([0-9A-Z]{4})"/>
    </format>
  </formats>
  <!-- Parsers section, parsers in this section are for XML databanks only
		 and are XPath based. For huge databanks you're probably better off
		 writing a Perl parser since XPath processing is relatively bit slow. -->
  <parsers>
    <parser chunk="/article" id="pmc">
      <index attr="true" name="id" type="string" unique="false" xpath="/article/front/article-meta/article-id[@pub-id-type=&quot;pmc&quot;]"/>
      <!-- really, in pmc id is not unique...  unique="true"  -->
      <index attr="false" name="pmid" type="string" unique="false" xpath="/article/front/article-meta/article-id[@pub-id-type=&quot;pmid&quot;]"/>
      <index attr="false" name="doi" type="string" unique="false" xpath="/article/front/article-meta/article-id[@pub-id-type=&quot;doi&quot;]"/>
      <index attr="false" name="subject" type="text" unique="false" xpath="/article/front/article-meta/article-categories/subj-group[@subj-group-type=&quot;heading&quot;]/subject"/>
      <index attr="true" name="title" type="text" unique="false" xpath="/article/front/article-meta/title-group/article-title"/>
      <index attr="false" name="authors" type="text" unique="false" xpath="/article/front/article-meta/contrib-group/contrib/name/*"/>
      <index attr="false" name="aff" type="text" unique="false" xpath="/article/front/article-meta/aff"/>
      <index attr="false" name="abstract" type="text" unique="false" xpath="/article/front/article-meta/abstract"/>
      <index attr="false" name="body" type="text" unique="false" xpath="/article/body"/>
    </parser>
    <!--	<parser id="gene" chunk="/Entrezgene-Set/Entrezgene">-->
    <!--		<index name='id'		type="string"	xpath='Entrezgene_track-info/Gene-track/Gene-track_geneid'-->
    <!--			   attr="true"		/>-->
    <!--		<index name='title'		type="text"		xpath='Entrezgene_prot/Prot-ref/Prot-ref_name/Prot-ref_name_E'-->
    <!--			   attr="true"	/>-->
    <!--		<index name='text'		type='text'		xpath='.//*' />-->
    <!--	</parser>-->
    <parser chunk="/interprodb/interpro" id="interpro">
      <index attr="true" name="id" type="string" unique="true" xpath="@id"/>
      <index attr="true" name="title" type="text" unique="false" xpath="name"/>
      <index attr="false" name="type" type="string" unique="false" xpath="@type"/>
      <index attr="false" name="name" type="string" unique="false" xpath="@short_name"/>
      <!-- Everything else (all nodes that are not 'name') -->
      <index attr="false" name="text" type="text" unique="false" xpath="*[name() != &quot;name&quot;]"/>
      <!-- All attributes -->
      <index attr="false" name="attr" type="text" unique="false" xpath=".//@*"/>
      <add-link db="@db" id="@dbkey" xpath="db_xref"/>
    </parser>
  </parsers>
  <!-- And finally, the databanks section -->
